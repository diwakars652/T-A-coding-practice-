import java.util.*;

class Square
{
   public static void main(String args[]) 
    {   
       
         Scanner s= new Scanner(System.in);
        
         System.out.println("Enter the side of the square:");
         
   int a= s.nextInt();
   
         int  perimeter=4*a;
     
   System.out.println("perimeter of Square is: " + perimeter);      
   }
}
