public class Demo {

	public static void main(String[] args) {
		
	     //   	Pattern No:- [01]
		
		int i,j;
		for (i=1;i<=5;i++) {
			for(j=1;j<=i;j++) {
				System.out.print("*");	
			}
			System.out.println();
		}
  }
}
		
  	Output:-          *
					           	* *
				          		* * *  
		          				* * * * 
			          			* * * * *
