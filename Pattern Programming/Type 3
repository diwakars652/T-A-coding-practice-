public class Demo {

	public static void main(String[] args) {
			
	//	Pattern No:- [03]
		
		int i,j,k;
		for(i=1;i<=5;i++) {
			for(k=1;k<=5-i+1;k++) {
				System.out.print(" ");
			}
			for(j=1;j<=i;j++) {
				System.out.print("*");
			}
			System.out.println();
		}
	}
}
		
		Output:-                        *
					      * *
					    * * *
					  * * * *
					* * * * *
