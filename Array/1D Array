// Create an array to store the ages of 5 students.



import java.util.*;


 class Dcoder
 {
   public static void main(String args[])
   { 
     int a[] = new int[5];
     Scanner sc = new Scanner(System.in);
     for(int i=0;i<=a.length-1;i++){
       System.out.println("The ages are : ");
       a[i] = sc.nextInt();
     }
     for (int i=0;i<=a.length-1;i++){
       System.out.println(a[i]);
     }
   }
 }

